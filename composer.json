{
    "name": "graham-campbell/cms-core",
    "description": "CMS Core Provides Some Core Functionality For Bootstrap CMS",
    "keywords": ["laravel", "framework", "cms core", "cms", "core", "CMS", "CMS Core", "CMS-Core", "Graham Campbell", "GrahamCampbell"],
    "license": "GNU Affero General Public License v3",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.0.*",
        "cartalyst/sentry": "2.1.*",
        "lightgear/asset": "1.2",
        "graham-campbell/core": "0.1.*",
        "graham-campbell/queuing": "0.1.*",
        "graham-campbell/htmlmin": "0.3.*",
        "graham-campbell/markdown": "0.2.*",
        "graham-campbell/binput": "0.3.*",
        "graham-campbell/passwd": "0.3.*",
        "graham-campbell/navigation": "0.1.*"
    },
    "require-dev": {
        "orchestra/testbench": "2.0.*",
        "mockery/mockery": "0.8.*",
        "phpunit/phpunit": "3.7.*",
        "sebastian/finder-facade": "1.1.*",
        "sebastianbergmann/phpcov": "1.1.0",
        "satooshi/php-coveralls": "dev-master",
        "symfony/config": "2.3.*",
        "symfony/event-dispatcher": "2.3.*",
        "symfony/stopwatch": "2.3.*",
        "symfony/yaml": "2.3.*",
        "guzzle/guzzle": "3.8.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "GrahamCampbell\\CMSCore\\": "src/",
            "GrahamCampbell\\Tests\\CMSCore": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
